<Window x:Class="myWPFPrj.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:myWPFPrj"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Loaded="Grid_Loaded">
        <Button x:Name="btn_Start" Content="Start" HorizontalAlignment="Left" Height="63" Margin="69,127,0,0" VerticalAlignment="Top" Width="100" Click="btn_Start_Click"/>
        <Button x:Name="btn_Cancel" Content="Cancel" HorizontalAlignment="Left" Height="63" Margin="69,213,0,0" VerticalAlignment="Top" Width="100" Click="btn_Cancel_Click"/>
        <TextBox x:Name="log" HorizontalAlignment="Left" Height="109" Margin="374,136,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="233"/>
    </Grid>
</Window>
